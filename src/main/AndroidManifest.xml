<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.netchosis.somthing.project2_phone2"
    android:versionCode="1" >
    android:versionName="1.0" >
    <uses-sdk android:minSdkVersion="9" />

    <uses-feature android:name="android.hardware.sip.voip" />

    <uses-permission android:name="android.permission.USE_SIP" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:name = "com.netchosis.somthing.project2_phone2.Settings">
        <receiver
            android:name="com.netchosis.somthing.project2_phone2.IncomingCallReceiver"
            android:label="Call Receiver" />
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/AppTheme"
        android:theme="@style/Theme.AppCompat.Light" >

        <activity
            android:name="com.netchosis.somthing.project2_phone2.MainActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.netchosis.somthing.project2_phone2.Getusers"
            android:label="@string/title_activity_getusers" >
        </activity>
        <activity
            android:name="com.netchosis.somthing.project2_phone2.UserProfile"
            android:label="@string/title_activity_user_profile">
        </activity>
        <activity
            android:name="com.netchosis.somthing.project2_phone2.IncomingCall"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_incoming_call"
            android:theme="@style/FullscreenTheme" >
        </activity>
        <activity android:name="com.netchosis.somthing.project2_phone2.Signup">
        </activity>

        <activity android:name="com.netchosis.somthing.project2_phone2.outboundcall"/>
        <service android:name="com.netchosis.somthing.project2_phone2.sipwork"/>

        <activity android:name="com.netchosis.somthing.project2_phone2.editprofile"/>
    </application>

</manifest>